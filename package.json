  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "version:patch": "npm version patch --no-git-tag-version && npm run changelog && git add . && git commit -m \"chore: bump version to $(npm run version:get)\" && git tag v$(npm run version:get)",
    "version:minor": "npm version minor --no-git-tag-version && npm run changelog && git add . && git commit -m \"chore: bump version to $(npm run version:get)\" && git tag v$(npm run version:get)",
    "version:major": "npm version major --no-git-tag-version && npm run changelog && git add . && git commit -m \"chore: bump version to $(npm run version:get)\" && git tag v$(npm run version:get)",
    "version:get": "node -p \"require('./package.json').version\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:patch": "npm run version:patch && git push && git push --tags",
    "release:minor": "npm run version:minor && git push && git push --tags",
    "release:major": "npm run version:major && git push && git push --tags",
    "precommit": "npm run lint",
    "prepare": "husky install"
  },
