{
  "name": "Trader's Journal",
  "short_name": "TradersJournal",
  "description": "Track your trades, analyze performance, and improve your trading strategy with AI-powered insights",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#000000",
  "theme_color": "#3B82F6",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en",
  "categories": ["finance", "business", "productivity"],
  "prefer_related_applications": false,
  "related_applications": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "icons": [
    {
      "src": "/default-avatar.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "Add Trade",
      "short_name": "Add Trade",
      "description": "Quickly add a new trade",
      "url": "/trade-records?action=add",
      "icons": [
        {
          "src": "/default-avatar.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View your trading dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/default-avatar.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View trading analytics",
      "url": "/trade-records",
      "icons": [
        {
          "src": "/default-avatar.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/images/landing/dashboard-preview.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Dashboard Overview"
    },
    {
      "src": "/images/landing/trade-records-preview.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Trade Records"
    },
    {
      "src": "/images/landing/analytics-preview.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Analytics Dashboard"
    }
  ],
  "file_handlers": [
    {
      "action": "/trade-records?action=import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+tradersjournal",
      "url": "/trade-records?action=import&url=%s"
    }
  ],
  "share_target": {
    "action": "/trade-records?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/csv", "application/json"]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "scope_extensions": [
    {
      "origin": "https://tradersjournal.por"
    }
  ],
  "capture_links": "existing-client-navigate",
  "display_override": ["standalone", "minimal-ui"]
} 